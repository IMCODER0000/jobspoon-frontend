{
	"name": "sveltekit-review-app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"mf:generate-types": "tsc --project tsconfig-mf.json && node scripts/zip-mf-types.cjs",
		"start": "npm run mf:generate-types && vite --config vite.mf.config.ts",
		"mf:dev": "vite --config vite.mf.config.ts",
		"mf:build": "npm run mf:generate-types && vite build --config vite.mf.config.ts",
		"mf:preview": "vite preview --config vite.mf.config.ts --port 5174"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@module-federation/vite": "^1.8.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-node": "^5.3.1",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.37.1",
		"@sveltejs/vite-plugin-svelte": "4.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.38.7",
		"svelte-check": "^4.0.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.20.0",
		"vite": "^5.4.10",
		"zip-a-folder": "^3.1.9"
	}
}
